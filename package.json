{
  "name": "@ApeironTsuka/vorpal",
  "version": "2.0.0",
  "description": "Node's first framework for building immersive CLI apps.",
  "main": "./lib/vorpal.js",
  "type": "module",
  "scripts": {
    "test": "mocha;"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ApeironTsuka/vorpal.git"
  },
  "keywords": [
    "api",
    "cli",
    "repl",
    "shell",
    "immersive",
    "framework",
    "app",
    "application",
    "command",
    "commander",
    "automated",
    "prompt",
    "inquirer"
  ],
  "author": "ApeironTsuka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ApeironTsuka/vorpal/issues"
  },
  "homepage": "https://github.com/ApeironTsuka/vorpal#readme",
  "devDependencies": {
    "bluebird": "^3.1.1",
    "mocha": "^10.4.0",
    "should": "^13.2.3",
    "vorpal-less": "0.0.13",
    "vorpal-repl": "^1.1.8",
    "xo": "^0.58.0"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "inquirer": "^9.2.22",
    "lodash": "^4.17.21",
    "log-update": "^6.0.0",
    "minimist": "^1.2.8",
    "node-localstorage": "^3.0.5",
    "strip-ansi": "^7.1.0",
    "wrap-ansi": "^9.0.0"
  },
  "xo": {
    "space": true,
    "rules": {
      "no-eval": 0,
      "no-unused-expressions": 0,
      "max-nested-callbacks": 0,
      "no-proto": 0,
      "wrap-iife": 0,
      "global-require": 0,
      "no-negated-condition": 0,
      "no-loop-func": 0,
      "no-implicit-coercion": 0,
      "no-use-extend-native/no-use-extend-native": 0,
      "no-undef": 0
    }
  },
  "engines": {
    "node": ">= 14.0.0"
  }
}
